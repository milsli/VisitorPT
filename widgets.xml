<?xml version="1.0" encoding='UTF-8' ?>
<layout orientation="horizontal">
	<layout orientation="vertical">
		<label value="Samochody" />
		<button name="buton" value="Button wyboru" method="onClicked" />
		<line name="samoch" value="Samochody do wyboru" method="onEditingFinished"/>
		<layout orientation="horizontal">
			<checkbox name="obobowy" value="osobowy" checked="true" method="onStateChanged" />
			<checkbox name="ciezarowy" value="ciężarowy" checked="true" method="onStateChanged" />
			<checkbox name="specjalny" value="specjalny" checked="false" method="onStateChanged" />
			<checkbox name="motor" value="jednoślad" checked="true" method="onStateChanged" />
		</layout>
		<layout orientation="horizontal">
			<radiobutton name="fiat" value="fiat" checked="true" method="onToggled" />
			<radiobutton name="merol" value="mercedes" checked="false" method="onToggled" />
			<radiobutton name="bmw" value="bmw" method="onToggled" />
			<radiobutton name="opel" value="opel" method="onToggled" />
		</layout>
		<combobox name="cars" method="onCurrentIndexChanged">
			<item>Maserati</item>
			<item>VW</item>
			<item>Peugeot</item>
			<item>Reanult</item>
		</combobox>
		<combobox name="types" method="onCurrentIndexChanged">
			<item>combi</item>
			<item>pick-up</item>
			<item>sedan</item>
			<item>hatchback</item>
		</combobox>
		<button name="wybor" value="Wybór"  method="onClicked" />
		<combobox name="wiek" method="onCurrentIndexChanged">
			<item>pon. 5</item>
			<item>pon. 15</item>
			<item>zabytki</item>
			<item>antyki</item>
		</combobox>
		<button name="poziom" value="Poziom"  method="onClicked" />
	</layout>
	<layout orientation="vertical">
		<list name="modele" value="lista" method="onCurrentIndexChanged">
			<item>corolla</item>
			<item>avensis</item>
			<item>sienna</item>
			<item>camry</item>
			<item>vectra</item>
			<item>bora</item>
			<item>tucson</item>
		</list>
		<label value="wartosc" />
		<line name="command" value="Komenda do wykonania" method="onEditingFinished"/>
		<combobox name="pochodzenie" method="onCurrentIndexChanged">
			<item>Japonia</item>
			<item>Korea</item>
			<item>USA</item>
			<item>Niemcy</item>
		</combobox>
	</layout>
</layout>

<?xml version="1.0" encoding='UTF-8' ?>
<layout orientation="horizontal">
	<layout orientation="vertical">
		<label value="Samochody" />
		<button value="Button wyboru" />
		<line value="Samochody do wyboru" />
		<layout orientation="horizontal">
			<checkbox value="osobowy" checked="true"/>
			<checkbox value="ciężarowy" checked="true"/>
			<checkbox value="specjalny" checked="false"/>
			<checkbox value="jednoślad" checked="true"/>
		</layout>
		<layout orientation="horizontal">
			<radiobutton value="fiat" checked="true" />
			<radiobutton value="mercedes" checked="false" />
			<radiobutton value="bmw" />
			<radiobutton value="opel" />
		</layout>
		<combobox name="cars" method="onCurrentIndexChanged">
			<item>Maserati</item>
			<item>VW</item>
			<item>Peugeot</item>
			<item>Reanult</item>
		</combobox>
		<combobox name="types" method="onCurrentIndexChanged">
			<item>combi</item>
			<item>pick-up</item>
			<item>sedan</item>
			<item>hatchback</item>
		</combobox>
		<button value="jakis" />
		<combobox name="wiek" method="onCurrentIndexChanged">
			<item>pon. 5</item>
			<item>pon. 15</item>
			<item>zabytki</item>
			<item>antyki</item>
		</combobox>
		<button value="jakis" />
	</layout>
	<layout orientation="vertical">
		<label value="wartosc" />
		<combobox name="pochodzenie" method="onCurrentIndexChanged">
			<item>Japonia</item>
			<item>Korea</item>
			<item>USA</item>
			<item>Niemcy</item>
		</combobox>
	</layout>
</layout>
